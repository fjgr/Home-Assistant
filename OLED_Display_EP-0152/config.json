{
    "name": "OLED Display EP-0152",
    "version": "1.0",
    "slug": "oled_display_ep0152",
    "description": "This plugin allows you to display on your OLED provided in Fan Expansion Board Plus 0.91 OLED V1.0 with the ssd1306 OLED.",
    "arch": [
        "armhf",
        "armv7",
        "aarch64"
    ],
    "log_level": "info",
    "startup": "services",
    "hassio_api": true,
    "auth_api": true,
    "ingress": false,
    "app_armor": true,
    "host_network": true,
    "privileged": [
        "SYS_ADMIN"
    ],
    "devices": [
        "/dev/i2c-1"
    ],
    "gpio": true,
    "stage": "experimental",
    "full_access": true,
    "url": "https://github.com/fjgr/Home-Assistant/new/main/OLED_Display_EP-0152",
    "boot": "manual",
    "options": {
        "Show_Splash_Screen": true,
        "Show_Network_Info": true,
        "Show_Storage_Info": true,
        "Show_Memory_Info": true,
        "Show_CPU_Info": true,
        "Temperature_Unit": "C",
        "Slide_Duration": 5,
        "Stop_Auto_Run": false
    },
    "schema": {
        "Show_Splash_Screen": "bool",
        "Show_Network_Info": "bool",
        "Show_Storage_Info": "bool",
        "Show_Memory_Info": "bool",
        "Show_CPU_Info": "bool",
        "Temperature_Unit": "list(C|F)?",
        "Slide_Duration": "int",
        "Stop_Auto_Run": "bool"
    }
}
